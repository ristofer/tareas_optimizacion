reset; 								# Resetear datos cada corrida
option solver cplex;				# CPLEX resolvera el problema
model svm_inf.mod;					# Cargar modelo (se asume que esta en la misma carpeta)
data sg6.dat;					# Cargar datos 
solve;								# Resolver el problema
print "Archivo de datos: sg6.dat" > "sg_inf03.txt"
print "z:" >> "sg_inf03.txt";
print z >> "sg_inf03.txt";						# Muestre variables de interes
print "W:" >> "sg_inf03.txt" ;
print {n in atributos} : W[n] >> "sg_inf03.txt";
print "CHI:" >> "sg_inf.txt" ;
print {i in elementos} : CHI[i] >> "sg_inf03.txt";
